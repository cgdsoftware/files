{
    "name": "laravel-enso/files",
    "description": "File manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "files",
        "file-manager",
        "file-uploader"
    ],
    "homepage": "https://github.com/laravel-enso/files",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Adrian Ocneanu",
        "email": "aocneanu@gmail.com",
        "homepage": "https://laravel-enso.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.4.0",
        "laravel-enso/core": "^5.0|^6.0",
        "laravel-enso/helpers": "^2.0",
        "laravel-enso/image-transformer": "^2.0",
        "laravel-enso/migrator": "^2.0",
        "laravel-enso/track-who": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Files\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Files\\AppServiceProvider",
                "LaravelEnso\\Files\\AuthServiceProvider",
                "LaravelEnso\\Files\\FileServiceProvider",
                "LaravelEnso\\Files\\UploadServiceProvider"
            ],
            "aliases": {
                "FileBrowser": "LaravelEnso\\Files\\Facades\\FileBrowser"
            }
        }
    }
}
